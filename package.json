{
  "name": "vibe-gx",
  "version": "2.1.8",
  "description": "A lightweight, high-performance Node.js web framework with optional C++ optimizations.",
  "type": "module",
  "main": "vibe.js",
  "types": "vibe.d.ts",
  "exports": {
    ".": {
      "import": "./vibe.js",
      "types": "./vibe.d.ts"
    }
  },
  "files": [
    "vibe.js",
    "vibe.d.ts",
    "utils/",
    "native/",
    "binding.gyp"
  ],
  "scripts": {
    "test": "node tests/unit.test.js && node tests/live.test.js",
    "test:all": "node tests/unit.test.js && node tests/live.test.js && node tests/scalability.test.js",
    "benchmark": "node tests/full-benchmark.js",
    "start": "node server.js",
    "build:native": "node-gyp rebuild",
    "install": "prebuild-install",
    "clean": "node-gyp clean"
  },
  "keywords": [
    "node",
    "framework",
    "http",
    "router",
    "middleware",
    "web",
    "backend",
    "fast",
    "native",
    "performance",
    "express-alternative"
  ],
  "author": "Nnamdi \"Joe\" Amaga",
  "license": "MIT",
  "dependencies": {
    "busboy": "^1.6.0",
    "node-addon-api": "^7.1.1",
    "node-gyp-build": "^4.8.4",
    "prebuild-install": "^7.1.3"
  },
  "optionalDependencies": {
    "node-gyp": "^10.3.1"
  },
  "engines": {
    "node": ">=18"
  },
  "gypfile": true,
  "binary": {
    "napi_versions": [
      3
    ],
    "host": "https://github.com/genesix/vibe/releases/download/"
  }
}